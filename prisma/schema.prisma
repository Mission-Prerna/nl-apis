  @@ignore
}

model assessment_visit_result_flatten {
  achievement     Int?
  block           String?   @db.VarChar
  competency      String?   @db.VarChar
  district        String?   @db.VarChar
  grade           Int?
  subject         String?   @db.VarChar
  total_questions Int?
  udise           String?   @db.VarChar
  view_type       String?   @db.VarChar
  id              Int       @id @default(autoincrement())
  assessment_id   Int
  is_nipun        Int?      @default(0)
  mentor_id       Int?
