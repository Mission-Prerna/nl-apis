  created_at      DateTime? @db.Date
  actor           String?   @db.VarChar
  student_session String?   @db.VarChar

  @@index([grade], map: "assessment_visit_result_flatten_ grade_idx")
  @@index([achievement])
  @@index([actor])
  @@index([assessment_id], map: "assessment_visit_result_flatten_ass_id_idx")
  @@index([assessment_type])
  @@index([block])
  @@index([competency])
  @@index([created_at])
  @@index([district])
  @@index([is_nipun])
  @@index([mentor_id])
  @@index([udise])
  @@index([view_type], map: "assessment_visit_result_flatten_view_tye_idx")
}

model assessment_visit_results {
  id               Int      @id @default(autoincrement())
  created_at       DateTime @default(now()) @db.Timestamptz(6)
  grade            Int
  subject          String
  module_result    String
  is_visited       Boolean  @default(false)
  mentor_id        Int?
  no_of_student    Int      @default(0)
